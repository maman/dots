! ----------------------------------------------------------------------------
! file:     ~/.Xdefaults
! author:   Achmad Mahardi - http://mahardi.heroku.com
! vim: :nu:ai:si:et:ts=4:sw=4:ft=xdefaults:
! ----------------------------------------------------------------------------

*customization:          -color
URxvt*termName:          rxvt-256color
URxvt*font:              -*-neep-medium-*-*-*-10-*-*-*-*-*-*-*
URxvt*boldFont:          -*-neep-medium-*-*-*-10-*-*-*-*-*-*-*
!URxvt.font:              xft:Pragmata for Powerline:pixelsize=9:antialias=false
!URxvt.boldFont:          xft:Pragmata for Powerline:pixelsize=9:antialias=false
URxvt.scrollBar:         false
URxvt.imLocale:          en_US.UTF-8
URxvt.saveLines:         512
URxvt.geometry:          90x20
URxvt.boldMode:          false
URxvt.inheritPixmap:     false
URxvt.jumpScroll:        true
URxvt.perl-ext-common:	 default,matcher
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]
URxvt.urlLauncher:	     firefox
URxvt.matcher.button:    1
URxvt*keysym.Home:       \033[7~
URxvt*keysym.End:        \033[8~
Urxvt.tabbed.tabbar-bg:	 7
Urxvt.tabbed.tabbar-fg:	 0
Urxvt.tabbed.tab-bg:	 5
Urxvt.tabbed.tab-fg:	 0
URxvt.internalBorder:    5
URxvt.colorBD:           #ffffff
URxvt.colorIT:           #bbbbbb
URxvt.colorUL:           #999999
URxvt.underlineColor:    #999999
URxvt.borderless:        true
URxvt.borderColor:       #151515

#include                 "/home/derp/.colors/railscasts.colors"
!#include                "/home/derp/.colors/urxvt.fix"

URxvt.depth:             32
URxvt.background:        #151515

!*background: 			 #151515
!*foreground: 			 #dddddd

alsamixer*scrollBar:     false

Xft.autohint:            0
Xft.lcdfilter:           lcddefault
Xft.dpi:                 96
Xft.antialias:           1
Xft.hinting:             1
Xft.hintstyle:           nohint
Xft.rgba:                rgb

